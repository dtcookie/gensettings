{
	"allowedScopes": [
		"environment"
	],
	"description": "Define a custom file name that should be used instead of the default RUM JavaScript file name, which is **ruxitagentjs**\\_\u003cfeature-hash\u003e\\_\u003cversion\u003e.js for auto-injection and **ruxitagent**\\_\u003cfeature-hash\u003e\\_\u003cversion\u003e.js for agentless RUM.",
	"displayName": "RUM JavaScript file name",
	"documentation": "",
	"dynatrace": "1",
	"enums": {},
	"maxObjects": 1,
	"multiObject": false,
	"properties": {
		"filename": {
			"constraints": [
				{
					"maxLength": 50,
					"minLength": 1,
					"type": "LENGTH"
				},
				{
					"customMessage": "Key may contain only alphanumeric characters. Characters must be from the Latin alphabet, with no diacritics; characters such as 'รถ' are not allowed.",
					"pattern": "^[0-9a-zA-Z]+$",
					"type": "PATTERN"
				},
				{
					"customMessage": "File name must not contain `ruxitagent`.",
					"customValidatorId": "rumJavascriptFilenameValidator",
					"type": "CUSTOM_VALIDATOR_REF"
				}
			],
			"default": "",
			"description": "",
			"displayName": "Custom file name",
			"documentation": "",
			"maxObjects": 1,
			"modificationPolicy": "DEFAULT",
			"nullable": false,
			"type": "text"
		}
	},
	"schemaGroups": [
		"group:web-and-mobile-monitoring.web-applications",
		"group:web-and-mobile-monitoring"
	],
	"schemaId": "builtin:rum.web.rum-javascript-file-name",
	"types": {},
	"version": "1.1.1"
}