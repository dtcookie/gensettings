			"modificationPolicy": "DEFAULT",
			"nullable": false,
			"type": "boolean"
		},
		"issuelabel": {
			"constraints": [
				{
					"maxLength": 100,
					"minLength": 1,
					"type": "LENGTH"
				}
			],
			"default": "Issue Tracker Label",
			"description": "Set a label to identify these issues, for example, `release_blocker` or `non-critical`",
			"displayName": "Issue label",
			"documentation": "",
			"maxObjects": 1,
			"modificationPolicy": "DEFAULT",
			"nullable": false,
			"type": "text"
		},
		"issuequery": {
			"constraints": [
				{
					"maxLength": 1000,
					"minLength": 1,
					"type": "LENGTH"
				}
			],
			"default": "",
