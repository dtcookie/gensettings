				},
				{
					"displayName": "GitLab",
					"value": "GITLAB"
				},
				{
					"displayName": "ServiceNow",
					"value": "SERVICENOW"
				},
				{
					"displayName": "Jira on-premises",
					"value": "JIRA_ON_PREMISE"
				},
				{
					"displayName": "Jira cloud",
					"value": "JIRA_CLOUD"
				}
			],
			"type": "enum"
		}
	},
