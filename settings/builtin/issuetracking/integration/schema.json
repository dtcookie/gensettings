			"type": {
				"$ref": "#/enums/IssueTrackerSystem"
			}
		},
		"password": {
			"constraints": [
				{
					"maxLength": 500,
					"minLength": 1,
					"type": "LENGTH"
				}
			],
			"description": "",
			"displayName": "Password",
			"documentation": "",
